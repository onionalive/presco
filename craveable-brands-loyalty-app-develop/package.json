{
  "name": "craveable-brands-loyalty-app",
  "version": "0.2.3",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "storybook": "storybook start -p 7007",
    "test:home": "mocha --require test/setup.js --compilers js:babel-core/register --timeout 0 ./app/components/home/Home.test.js",
    "test:functions": "mocha --compilers js:babel-core/register --timeout 0 ./test/*.test.js --watch",
    "test:accounts": "mocha --compilers js:babel-core/register --timeout 0 ./test/Accounts.test.js --watch",
    "test:transactions": "mocha --compilers js:babel-core/register --timeout 0 ./test/Transactions.test.js --watch",
    "test:tracking": "mocha --compilers js:babel-core/register --timeout 0 ./test/Tracking.test.js --watch",
    "test:card": "mocha --compilers js:babel-core/register --timeout 0 ./test/Card.test.js --watch",
    "test:dollars": "mocha --compilers js:babel-core/register --timeout 0 ./test/Dollars.test.js --watch",
    "test:feedback": "mocha --compilers js:babel-core/register --timeout 0 ./test/Feedback.test.js --watch",
    "test:locations": "mocha --compilers js:babel-core/register --timeout 0 ./test/Locations.test.js --watch",
    "test:navigation": "mocha --compilers js:babel-core/register --timeout 0 ./test/Navigation.test.js --watch",
    "test:promotions": "mocha --compilers js:babel-core/register --timeout 0 ./test/PromotionsOffers.test.js --watch",
    "test:products": "mocha --compilers js:babel-core/register --timeout 0 ./test/Products.test.js --watch",
    "docs": "open ./doc/index.html",
    "flow": "flow",
    "bundle:fix": "cd ./node_modules/js-base64 && npm i babel-preset-es2015 && cd ../..",
    "bundle:ios": "react-native bundle --entry-file index.ios.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "dev:clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "dev:reset": "rm -rf node_modules && npm install && react-native link && npm run dev:clean",
    "clean:ios": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "unlink": "unlink ios && unlink android",
    "link:oporto": "ln -s ./ios_oporto ./ios && ln -s ./android_oporto ./android",
    "link:redrooster": "ln -s ./brands/redrooster/ios . && ln -s ./brands/redrooster/android ."
  },
  "dependencies": {
    "apisauce": "^0.14.3",
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-preset-airbnb": "^2.4.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "chalk": "^2.1.0",
    "classnames": "^2.2.5",
    "deepmerge": "^2.0.1",
    "enzyme": "^2.2.0",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "haversine": "^1.1.0",
    "imgix-core-js": "^1.1.1",
    "lodash": "^4.17.4",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "luxon": "^0.2.0",
    "mocha": "^4.0.0",
    "moment": "^2.20.1",
    "moxios": "^0.4.0",
    "node-emoji": "^1.8.1",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0-alpha.12",
    "react-native": "0.51.0",
    "react-native-actionsheet": "^2.3.0",
    "react-native-animatable": "^1.2.4",
    "react-native-camera": "^0.10.0",
    "react-native-check-box": "^2.0.2",
    "react-native-device-info": "^0.12.1",
    "react-native-easy-grid": "^0.1.16",
    "react-native-exact-target": "^0.3.1",
    "react-native-firebase": "^3.2.0",
    "react-native-iphone-x-helper": "^1.0.1",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-maps": "^0.17.1",
    "react-native-material-textfield": "^0.11.0",
    "react-native-open-maps": "^0.1.1",
    "react-native-permissions": "^1.0.6",
    "react-native-push-notification": "^3.0.1",
    "react-native-qrcode-svg": "^5.0.3",
    "react-native-segmented-control-tab": "^3.2.1",
    "react-native-sensitive-info": "^5.1.0",
    "react-native-svg": "^5.5.1",
    "react-native-swiper": "^1.5.13",
    "react-navigation": "1.0.0",
    "react-navigation-redux-debouncer": "0.0.2",
    "react-navigation-redux-helpers": "^1.0.0",
    "react-redux": "^5.0.6",
    "react-redux-firebase": "^1.5.1",
    "redux": "^3.7.2",
    "redux-firestore": "^0.1.0",
    "redux-logger": "^3.0.6",
    "redux-loop": "^4.1.0",
    "redux-persist": "^5.4.0",
    "redux-persist-sensitive-storage": "^1.0.0",
    "redux-persist-transform-immutable": "^4.3.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "sinon": "^4.0.1",
    "titleize": "^1.0.0"
  },
  "devDependencies": {
    "@storybook/react-native": "^3.2.12",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "^4.0.0",
    "flow-bin": "^0.49.1",
    "jest": "21.2.1",
    "react-native-mock": "^0.3.1",
    "react-test-renderer": "^16.0.0-alpha.12"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
