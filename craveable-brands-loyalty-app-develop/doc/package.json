{
  "name": "craveable-brands-loyalty-app-doc",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "storybook": "storybook start -p 7007",
    "test:home": "mocha --require test/setup.js --compilers js:babel-core/register --timeout 0 ./app/components/home/Home.test.js",
    "test:functions": "mocha --compilers js:babel-core/register --timeout 0 ./test/*.test.js --watch",
    "test:accounts": "mocha --compilers js:babel-core/register --timeout 0 ./test/Accounts.test.js --watch",
    "docs": "open ./doc/index.html",
    "flow": "flow",
    "oporto": "rm -rf $TMPDIR/react-* && watchman watch-del-all && THEME=oporto node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "redrooster": "rm -rf $TMPDIR/react-* && watchman watch-del-all && THEME=redrooster node node_modules/react-native/local-cli/cli.js start -- --reset-cache"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-preset-airbnb": "^2.4.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "chalk": "^2.1.0",
    "enzyme": "^2.2.0",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "mocha": "^4.0.0",
    "moxios": "^0.4.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0-alpha.12",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0-alpha.12",
    "react-native": "0.48.4",
    "react-native-orientation": "^3.1.0",
    "react-native-swiper": "^1.5.13",
    "react-navigation": "^1.0.0-beta.13",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-loop": "^4.1.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "sinon": "^4.0.1"
  },
  "devDependencies": {
    "@storybook/react-native": "^3.2.12",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "^4.0.0",
    "flow-bin": "^0.49.1",
    "jest": "21.2.1",
    "react-native-mock": "^0.3.1",
    "react-test-renderer": "^16.0.0-alpha.12"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
