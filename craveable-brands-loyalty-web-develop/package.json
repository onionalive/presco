  "scripts": {
    "start-js": "node scripts/start.js",
    "start": "NODE_PATH=src/ npm-run-all -p watch start-js",
    "watch": "gulp watch",
    "now": "npm run build && now --docker -e NODE_ENV=production",
    "build": "gulp build && NODE_PATH=src/ node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "mocha:home": "NODE_ENV=development mocha --require test/setup.js --compilers js:babel-register --timeout 0 ./src/components/home/Home.test.js",
    "mocha": "mocha --recursive ./src/components/**/*.mocha.js",
    "start-storybook": "start-storybook -p 6006",
    "storybook": "NODE_PATH=src/ npm-run-all -p watch start-storybook",
    "docs": "open ./doc/index.html",
    "deploy": "gulp build && node scripts/build.js && now --alias <name>",
    "surge": "gulp build && node scripts/build.js && surge build/ <name>.surge.sh",
    "build-storybook": "gulp build && build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-links": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chalk": "^2.1.0",
    "enzyme": "^2.9.1",
    "enzyme-adapter-react-15": "^1.0.1",
