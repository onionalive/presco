	<head>
		{% include "partials/meta.njk" %}
		<link rel="icon" type="img/png" href="img/favicon.ico">
		<link rel="stylesheet" href="css/main.css">
		<% if (typekitID != '') { %>
		<script src="https://use.typekit.net/<%= typekitID %>.js"></script>
		<script>try{Typekit.load({ async: true });}catch(e){}</script>
		<% } %>
	</head>
	<body>
		<div class="container">
			{% include "partials/header.njk" %}

			{% block content %}
			{% endblock %}

			<% if (slick) { %>
				<div class="slider">
					<div>
						<h2>this is a slick carousel</h2>
					</div>
					<div>
						<p>you probably should delete this</p>
					</div>
				</div>
			<% } %>
			{% include "partials/analytics.njk" %}
